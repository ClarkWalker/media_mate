{
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"source": {
		"include": ["app/", "app/renderjs", "app/lib"],
		"exclude": "app/node_modules/"
	},
	"plugins": [
		"plugins/markdown",
		"node_modules/jsdoc-strip-async-await"
	],
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"template": "templates/default",
		"encoding": "utf8",
		"destination": "./docs/"
	}
}
